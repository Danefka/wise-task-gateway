type Profile {
    id : ID!
    email: String!
    profilePassword: String!
    firstName: String!
    lastName: String!
    patronymic: String
    profileRole: Role!
    studentGroup: String
    studentCourse: Int
}

enum Role {
    STUDENT
    TEACHER
    CAPTAIN
    ADMIN
}

type Query {
    profiles: [Profile]!
}